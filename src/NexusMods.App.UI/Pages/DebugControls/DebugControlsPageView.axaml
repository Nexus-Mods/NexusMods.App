                                            </MenuFlyout>
                                        </SplitButton.Flyout>
                                    </SplitButton>
                                    <SplitButton Classes="StandardSplitButton" IsEnabled="False">
                                        <TextBlock Text="Disabled" />
                                    </SplitButton>
                                </StackPanel>
