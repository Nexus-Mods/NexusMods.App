                                    <controls:StandardButton Text="Secondary-Weak" ShowIcon="Right" Type="Secondary"
                                                             Fill="Weak" />
                                    <controls:StandardButton Text="Secondary-None" ShowIcon="Right" Type="Secondary"
                                                             Fill="None" />
                                    <controls:StandardButton Text="Primary-Strong" ShowIcon="Right" Type="Primary"
                                                             Fill="Strong" />
                                    <controls:StandardButton Text="Primary-Weak" ShowIcon="Right" Type="Primary"
                                                             Fill="Weak" />
                                    <controls:StandardButton Text="Extra Small" Size="ExtraSmall" />
                                    <controls:StandardButton Text="Extra Small" Size="ExtraSmall" ShowIcon="Both" />
                                </StackPanel>
