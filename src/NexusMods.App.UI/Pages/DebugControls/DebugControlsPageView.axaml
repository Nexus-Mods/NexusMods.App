    <Border Background="{StaticResource SurfaceLowBrush}">
        <TabControl>
            <TabItem>
                <TabItem.Header>
                    <StackPanel Classes="TabItemHeaderStack">
                        <TextBlock Text="Controls" />
                    </StackPanel>
                </TabItem.Header>
                <Border Padding="8">
                    <ScrollViewer>
                        <StackPanel>
                            <StackPanel Orientation="Horizontal" Spacing="16">
                                <StackPanel Spacing="8">
                                    <TextBlock Text="Button" />
                                    <Button>Enabled</Button>
                                    <Button IsEnabled="False">Disabled</Button>
                                </StackPanel>
                                <StackPanel Spacing="8">
                                    <TextBlock Text="SplitButton" />
                                    <SplitButton>
                                        <TextBlock Text="Enabled" />
                                        <SplitButton.Flyout>
                                            <MenuFlyout>
                                                <MenuItem Header="Install" />
                                                <MenuItem Header="Advanced Install" />
                                            </MenuFlyout>
                                        </SplitButton.Flyout>
                                    </SplitButton>
                                    <SplitButton IsEnabled="False">
                                        <TextBlock Text="Disabled" />
                                    </SplitButton>
                                </StackPanel>
                                <StackPanel Spacing="8">
                                    <TextBlock Text="ComboBox" />
                                    <ComboBox PlaceholderText="Placeholder">
                                        <ComboBoxItem>Item 1</ComboBoxItem>
                                        <ComboBoxItem>Item 2</ComboBoxItem>
                                    </ComboBox>
                                    <ComboBox SelectedIndex="0">
                                        <ComboBoxItem>Item 1</ComboBoxItem>
                                        <ComboBoxItem>Item 2</ComboBoxItem>
                                    </ComboBox>
                                    <ComboBox PlaceholderText="Placeholder" IsEnabled="False">
                                        <ComboBoxItem>Item 1</ComboBoxItem>
                                        <ComboBoxItem>Item 2</ComboBoxItem>
                                    </ComboBox>
                                    <ComboBox IsEnabled="False" SelectedIndex="0">
                                        <ComboBoxItem>Item 1</ComboBoxItem>
                                    </ComboBox>
                                </StackPanel>
                                <StackPanel Spacing="8">
                                    <TextBlock Text="ToggleSwitch" />
                                    <ToggleSwitch />
                                    <ToggleSwitch IsChecked="True" />
                                    <ToggleSwitch IsChecked="{x:Null}" IsThreeState="True" />
                                    <ToggleSwitch IsEnabled="False" />
                                    <ToggleSwitch IsChecked="True" IsEnabled="False" />
                                </StackPanel>
                                <StackPanel Spacing="8">
                                    <TextBlock Text="CheckBox" />
                                    <CheckBox>Enabled</CheckBox>
                                    <CheckBox IsChecked="True">Checked</CheckBox>
                                    <CheckBox IsChecked="{x:Null}" IsThreeState="True">3-State</CheckBox>
                                    <CheckBox IsEnabled="False">Disabled</CheckBox>
                                    <CheckBox IsChecked="True" IsEnabled="False">Checked</CheckBox>
                                    <CheckBox IsChecked="{x:Null}" IsEnabled="False">3-State</CheckBox>
                                </StackPanel>
                                <StackPanel Spacing="8">
                                    <TextBlock Text="RadioButton" />
                                    <RadioButton>Enabled</RadioButton>
                                    <RadioButton IsChecked="True">Checked</RadioButton>
                                    <RadioButton IsEnabled="False">Disabled</RadioButton>
                                    <RadioButton IsChecked="True" IsEnabled="False">Checked</RadioButton>
                                </StackPanel>
                                <StackPanel Spacing="8">
                                    <TextBlock Text="TextBox" />
                                    <TextBox>Enabled</TextBox>
                                    <TextBox Watermark="Placeholder" />
                                    <TextBox IsEnabled="False">Disabled</TextBox>
                                </StackPanel>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Spacing="16">
                                <StackPanel Spacing="8">
                                    <TextBlock Text="StandardButton" />
                                    <controls:StandardButton Text="Default" />
                                    <controls:StandardButton Text="Both Icons" ShowIcon="Both" />
                                    <controls:StandardButton Text="Medium" ShowIcon="Both" Size="Medium" />
                                    <controls:StandardButton Text="Secondary-Strong" ShowIcon="Right" Type="Secondary"
