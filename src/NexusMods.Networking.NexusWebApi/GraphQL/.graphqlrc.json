{
  "schema": "schema.graphql",
  "documents": "**/*.graphql",
  "extensions": {
    "endpoints": {
      "NexusGraphQLClient": {
        "url": "https://api.nexusmods.com/v2/graphql"
      }
    },
    "strawberryShake": {
      "name": "NexusGraphQLClient",
      "url": "https://api.nexusmods.com/v2/graphql",
      "records": {
        "inputs": false,
        "entities": false
      },
      "transportProfiles": [
        {
          "default": "Http",
          "subscription": "WebSocket"
        }
      ]
    }
  }
}
