  "LoggingSettings": {
    "FilePath": "{EntryFolder}/Logs/nexusmods.app.current.log",
    "ArchiveFilePath": "{EntryFolder}/Logs/nexusmods.app.{##}.log",
    "MaxArchivedFiles": 10
  }
