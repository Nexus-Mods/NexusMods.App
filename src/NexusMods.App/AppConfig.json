{
  "DataModelSettings": {
    "DataStoreFilePath": "{EntryFolder}/DataModel.sqlite",
    "IpcDataStoreFilePath": "{EntryFolder}/DataModel_IPC.sqlite",
    "ArchiveLocations": [
      "{EntryFolder}/Archives"
    ],
    "MaxHashingJobs": -1,
    "LoadoutDeploymentJobs": -1,
