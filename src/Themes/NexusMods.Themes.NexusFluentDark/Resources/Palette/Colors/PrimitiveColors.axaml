<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--
        Color Primitives (Layer 1):
        These base colours define the building blocks of our colour system.
        They will not be used directly in Themes or Styles as their descriptive names make them difficult to change in the future.
        They will instead be accessed from Layer 2 (Brand Palette) where their names are aliased.
    -->


    <!-- Zinc -->
    <Color x:Key="PrimitiveZinc950">#09090B</Color>
    <Color x:Key="PrimitiveZinc900">#18181B</Color>
    <Color x:Key="PrimitiveZinc800">#27272A</Color>
    <Color x:Key="PrimitiveZinc700">#3F3F46</Color>
    <Color x:Key="PrimitiveZinc600">#52525B</Color>
    <Color x:Key="PrimitiveZinc500">#71717A</Color>
    <Color x:Key="PrimitiveZinc400">#A1A1AA</Color>
    <Color x:Key="PrimitiveZinc300">#D4D4D8</Color>
    <Color x:Key="PrimitiveZinc200">#E4E4E7</Color>
    <Color x:Key="PrimitiveZinc100">#F4F4F5</Color>
    <Color x:Key="PrimitiveZinc50">#FAFAFA</Color>

