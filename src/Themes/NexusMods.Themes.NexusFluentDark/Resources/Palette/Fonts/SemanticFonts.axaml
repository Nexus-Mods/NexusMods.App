<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Include the Primitive fonts -->
    <ResourceDictionary.MergedDictionaries>
        <ResourceInclude Source="/Resources/Palette/Fonts/PrimitiveFonts.axaml" />
    </ResourceDictionary.MergedDictionaries>


    <StaticResource x:Key="FontHeadlinesRegular" ResourceKey="InterRegular"/>
    <StaticResource x:Key="FontHeadlinesSemiBold" ResourceKey="InterSemiBold"/>
    <StaticResource x:Key="FontHeadlinesBold" ResourceKey="InterSemiBold"/>

    <StaticResource x:Key="FontBodyRegular" ResourceKey="InterRegular"/>
    <StaticResource x:Key="FontBodySemiBold" ResourceKey="InterSemiBold"/>
    <StaticResource x:Key="FontBodyBold" ResourceKey="InterSemiBold"/>

</ResourceDictionary>

